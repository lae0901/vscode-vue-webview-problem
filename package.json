{
	"name": "vuewebview",
	"description": "use vue within webview",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vue.webview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vue.webview",
				"title": "use vue within webview",
				"category": "vue webview"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2",
		"vscode": "^1.1.29"
	}
}
